@model IEnumerable<tanyaradzwaStudyGroup.Models.StudentList>

@{
    ViewBag.Title = "Student Details";
}
<link rel="stylesheet" type="text/css" href="~/Content/CSS_Students.css" />

<h2>Student Details</h2>




<table class="table">
    <tr class="headings">
        <th>@Html.DisplayNameFor(model => model.StudentNumber)</th>
        <th>@Html.DisplayNameFor(model => model.Name)</th>
        <th>@Html.DisplayNameFor(model => model.Surname)</th>
        <th>@Html.DisplayNameFor(model => model.Email_Address)</th>
        <th>@Html.DisplayNameFor(model => model.myLink)</th>
    </tr>
    @foreach (var item in Model)
    {
        <tr>
            <td>@item.StudentNumber</td>
            <td>@item.Name</td>
            <td>@item.Surname</td>
            <td>@item.Email_Address</td>
            <td><a href="@Url.Content(item.myLink)" target="_blank">LINK</a></td>
        </tr>
    }
</table>
